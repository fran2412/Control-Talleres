        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <local:FormularioAlumnoUserControl Grid.Row="0" Margin="40,0,40,0"/>
