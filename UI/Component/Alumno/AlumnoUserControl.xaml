             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ControlTalleresMVP.UI.Component.Alumno"
             mc:Ignorable="d" 
             d:DesignHeight="1000" d:DesignWidth="800">
